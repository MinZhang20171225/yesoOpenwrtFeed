config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option txpower '30'
	option country 'US'
	option disabled '0'
	option channel '1'

config wifi-iface
	option network 'wwan'
	option ssid 'YE'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option bssid 'C4:04:15:1B:CA:2F'
	option key '7596682821'

